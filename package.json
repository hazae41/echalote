{
  "type": "module",
  "name": "@hazae41/echalote",
  "version": "0.2.30",
  "author": "hazae41",
  "license": "MIT",
  "description": "Zero-copy Tor protocol for the web",
  "homepage": "https://github.com/hazae41/echalote",
  "repository": "github:hazae41/echalote",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "node ./dist/test/index.test.mjs",
    "bench": "node ./dist/bench/index.bench.mjs",
    "fallbacks": "cd ./tools/fallbacks && deno run -A ./parser.ts && deno fmt ./fallbacks.json",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@hazae41/arrays": "^1.0.0",
    "@hazae41/binary": "^1.2.10",
    "@hazae41/bitset": "^1.0.1",
    "@hazae41/bytes": "^1.0.4",
    "@hazae41/cadenas": "^0.2.10",
    "@hazae41/cascade": "^1.0.0",
    "@hazae41/fleche": "^1.1.16",
    "@hazae41/future": "^1.0.3",
    "@hazae41/morax": "^1.0.20",
    "@hazae41/mutex": "^1.0.2",
    "@hazae41/paimon": "^1.0.15",
    "@hazae41/piscine": "^1.0.2",
    "@hazae41/x509": "^1.0.24",
    "@hazae41/zepar": "^1.0.13"
  },
  "devDependencies": {
    "@hazae41/berith": "^1.1.40",
    "@hazae41/deimos": "^1.0.6",
    "@hazae41/ed25519": "^1.0.0",
    "@hazae41/phobos": "^1.0.10",
    "@hazae41/x25519": "^1.0.0",
    "@noble/curves": "^0.9.0",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.15.7",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "typescript": "^5.0.2"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "tor",
    "onion",
    "protocol",
    "browser",
    "buffer",
    "stream",
    "streaming",
    "http",
    "https",
    "privacy",
    "tls",
    "packets",
    "binary",
    "encoding",
    "decoding",
    "compression",
    "cryptography",
    "crypto",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
}

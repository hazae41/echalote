    "@hazae41/binary": "^1.1.2",
    "@hazae41/cadenas": "^0.0.3",
    "@hazae41/fleche": "^1.0.3",
    "@hazae41/foras": "^2.0.3",
    "@hazae41/morax": "^1.0.15",
    "@hazae41/paimon": "^1.0.11",
    "@hazae41/x509": "^1.0.15",
    "@hazae41/zepar": "^1.0.8",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@hazae41/phobos": "^1.0.7",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.11.18",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",

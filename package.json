    "@hazae41/base16": "^1.0.17",
    "@hazae41/base64": "^1.0.10",
    "@hazae41/binary": "^1.3.4",
    "@hazae41/bitset": "^1.0.1",
    "@hazae41/box": "^1.0.14",
    "@hazae41/bytes": "^1.2.9",
    "@hazae41/cadenas": "^0.4.0",
    "@hazae41/cascade": "^2.2.2",
    "@hazae41/cursor": "^1.2.3",
    "@hazae41/ed25519": "^2.1.10",
    "@hazae41/fleche": "^1.4.4",
    "@hazae41/future": "^1.0.3",
    "@hazae41/kcp": "^1.1.2",
    "@hazae41/mutex": "^2.0.0",
    "@hazae41/option": "^1.0.27",
    "@hazae41/paimon": "^1.1.3",
    "@hazae41/plume": "^2.4.2",
    "@hazae41/result": "^1.2.0",
    "@hazae41/sha1": "^1.1.12",
    "@hazae41/signals": "^1.0.1",
    "@hazae41/smux": "^1.1.1",
    "@hazae41/x25519": "^2.1.4",
    "@hazae41/x509": "^1.2.8",
    "@hazae41/zepar": "^1.1.6"
  },
  "devDependencies": {
    "@hazae41/deimos": "^1.0.6",
    "@hazae41/phobos": "^1.0.10",
    "@hazae41/saumon": "^0.2.7",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-externals": "^7.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/src/index.mjs",
      "require": "./dist/cjs/src/index.cjs"
    }
  },
  "keywords": [
    "tor",
    "onion",
    "protocol",
    "browser",
    "buffer",
    "stream",
    "streaming",
    "http",
    "https",
    "privacy",
    "tls",
    "packets",
    "binary",
    "encoding",
    "decoding",
    "compression",
    "cryptography",
    "crypto",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
